{
  "name": "double-cheese",
  "displayName": "double-cheese",
  "publisher": "MAKinteract",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/makinteract/double-cheese"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.63.0"
  },
  "enabledApiProposals": [
    "editorInsets"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:cpp"
  ],
  "main": "./out/main.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "type": "webview",
          "id": "double-cheese.view",
          "name": "Double-Cheese Playground"
        }
      ]
    },
    "commands": [
      {
        "command": "double-cheese.configureConnection",
        "category": "doublecheese",
        "title": "Configure and connect to serial"
      },
      {
        "command": "double-cheese.connect",
        "category": "doublecheese",
        "title": "Connect to serial"
      },
      {
        "command": "double-cheese.disconnect",
        "category": "doublecheese",
        "title": "Disconnect from serial"
      },
      {
        "command": "double-cheese.compileUpload",
        "category": "doublecheese",
        "title": "Compile and upload sketch"
      },
      {
        "command": "double-cheese.clearAnnotations",
        "category": "doublecheese",
        "title": "Remove annotations //? from code"
      },
      {
        "command": "double-cheese.toggleHighlight",
        "category": "doublecheese",
        "title": "Toggle highlight"
      },
      {
        "command": "double-cheese.changeServer",
        "category": "doublecheese",
        "title": "Change server"
      }
    ],
    "languages": [
      {
        "id": "ino",
        "extensions": [
          ".ino",
          ".cpp"
        ]
      }
    ],
    "grammars": [
      {
        "language": "ino",
        "path": "./syntaxes/ino.tmLanguage.json",
        "scopeName": "source.ino"
      }
    ],
    "configurationDefaults": {
      "editor.formatOnSave": false,
      "editor.tokenColorCustomizations": {
        "textMateRules": [
          {
            "scope": "special.functions",
            "settings": {
              "fontStyle": "underline",
              "foreground": "#EB64B9"
            }
          }
        ]
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./ && npm run compile_grammar",
    "watch": "tsc -watch -p ./",
    "test": "jest --watchAll --verbose",
    "check_license": "npx license-checker --summary",
    "compile_grammar": "npx jison ./src/parser/grammar.jison -o ./src/parser/grammar.js && cp -prf ./src/parser/grammar.js ./out/parser",
    "compile_expression_grammar": "npx jison ./src/parser/expressions.jison -o ./src/parser/expressions.js && cp -prf ./src/parser/expressions.js ./out/parser",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "package": "npx vsce package"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@types/glob": "^7.1.4",
    "@types/jest": "^27.0.3",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.63.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.37.0",
    "typescript": "^4.4.4",
    "vsce": "^2.9.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@types/socket.io-client": "^3.0.0",
    "crypto-js": "^4.1.1",
    "fs": "^0.0.1-security",
    "jest": "^27.4.5",
    "rxjs": "^7.8.0",
    "sirv-cli": "^1.0.0",
    "socket.io": "^4.6.0",
    "socket.io-client": "^4.4.0"
  }
}
