{
  "name": "double-cheese",
  "displayName": "double-cheese",
  "publisher": "MAKinteract",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:double-cheese.view",
    "onCommand:double-cheese.configureConnection",
    "onCommand:double-cheese.connect",
    "onCommand:double-cheese.disconnect",
    "onCommand:double-cheese.compileUpload",
    "onCommand:double-cheese.clearAnnotations",
    "onCommand:double-cheese.toggleHighlight",
    "onLanguage:cpp"
  ],
  "main": "./out/main.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "type": "webview",
          "id": "double-cheese.view",
          "name": "Double Cheese"
        }
      ]
    },
    "commands": [
      {
        "command": "double-cheese.configureConnection",
        "category": "doublecheese",
        "title": "Configure and connect to serial"
      },
      {
        "command": "double-cheese.connect",
        "category": "doublecheese",
        "title": "Connect to serial"
      },
      {
        "command": "double-cheese.disconnect",
        "category": "doublecheese",
        "title": "Disconnect from serial"
      },
      {
        "command": "double-cheese.compileUpload",
        "category": "doublecheese",
        "title": "Compile and upload sketch"
      },
      {
        "command": "double-cheese.clearAnnotations",
        "category": "doublecheese",
        "title": "Remove annotations //? from code"
      },
      {
        "command": "double-cheese.toggleHighlight",
        "category": "doublecheese",
        "title": "Toggle highlight"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./ && npm run compile_grammars",
    "watch": "tsc -watch -p ./",
    "test": "jest --watchAll --verbose",
    "check_license": "npx license-checker --summary",
    "compile_grammars": "npx jison ./src/parser/grammar.jison -o ./src/parser/grammar.js && cp -prf ./src/parser/grammar.js ./out/parser",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@types/glob": "^7.1.4",
    "@types/jest": "^27.0.3",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.63.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.37.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@types/socket.io-client": "^3.0.0",
    "crypto-js": "^4.1.1",
    "fs": "^0.0.1-security",
    "jest": "^27.4.5",
    "sirv-cli": "^1.0.0",
    "socket.io-client": "^4.4.0"
  }
}
